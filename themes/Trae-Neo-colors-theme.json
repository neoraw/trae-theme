{
  "name": "Trae Neo",
  "type": "dark",

  "palette": {
    "bgLight": "#222427",
    "bg": "#1a1b1d",
    "accent_focus": "#323336",
    "accent-hover": "#242628",
    "text": "#fff",
    "muted": "#ccced6",
    "html": "#f2858c",
    "keyword": "#b38cff",
    "variable": "#ded47e",
    "punctuation": "#ded47e",
    "parameter": "#ded47e",
    "string": "#82d99f",
    "function": "#f29d79",
    "constante": "#80bbff",
    "number": "#f48cca",
    "class": "#b38cff",
    "comment": "#737780",
    "operator": "#fff",
    "warning": "#cca700",
    "error": "#ff6464"
  },

  "colors": {
    "focusBorder": "#323336",
    "list.activeSelectionBackground": "#323336",
    "list.activeSelectionForeground": "#FFFFFF",
    "editor.selectionBackground": "#323336",
    "statusBar.background": "#323336",
    "activityBar.activeBorder": "#323336",

    "list.hoverBackground": "#242628",
    "list.hoverForeground": "#FFFFFF",
    "editorHoverWidget.background": "#242628",
    "editorHoverWidget.foreground": "#FFFFFF",
    "editorHoverWidget.border": "#323336",

    "editorCursor.foreground": "#FFFFFF",
    "terminalCursor.foreground": "#FFFFFF",

    "activityBar.background": "#222427",
    "sideBar.background": "#1a1b1d",

    "editor.background": "#1a1b1d",
    "panel.background": "#1a1b1d",
    "tab.activeBackground": "#1a1b1d",
    "tab.inactiveBackground": "#222427",

    "editorGroup.background": "#1a1b1d",
    "editorGroupHeader.tabsBackground": "#1a1b1d",

    "sideBar.border": "#232528",
    "editorGroup.border": "#2a2c2f",
    "editorGroupHeader.tabsBorder": "#232528",
    "editorGroupHeader.border": "#232528",

    "foreground": "#ffffff",
    "descriptionForeground": "#ccced6",
    "sideBar.foreground": "#ccced6",
    "list.inactiveSelectionForeground": "#ffffff",
    "list.focusForeground": "#ffffff",
    "editorLineNumber.foreground": "#5c5e61",
    "editorLineNumber.activeForeground": "#ccced6"
  },

  "tokenColors": [
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#b38cff"
      }
    },
    {
      "name": "Imports & Exports",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "meta.import",
        "meta.export"
      ],
      "settings": { "foreground": "#f48c83" }
    },
    {
      "name": "Variables, Parameters and Punctuation",
      "scope": [
        "variable",
        "variable.other.readwrite",
        "variable.other",
        "variable.language",
        "variable.parameter",
        "meta.parameter",
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.section",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#ded47e"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function",
        "meta.function.method",
        "support.method",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#f29d79"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.regexp",
        "string.interpolated",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#82d99f"
      }
    },

    {
      "name": "Constants",
      "scope": [
        "variable.other.constant",
        "variable.constant",
        "meta.definition.variable constant",
        "meta.const-variable",
        "meta.var.expr.ts",
        "meta.variable.ts",
        "meta.variable.declaration",
        "meta.variable",
        "support.constant"
      ],
      "settings": {
        "foreground": "#80bbff"
      }
    },

    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "meta.property",
        "variable.other.member",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#e4e6eb"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "storage.type.cs",
        "storage.type.annotation",
        "meta.type",
        "meta.return-type",
        "meta.type.annotation",
        "meta.interface",
        "meta.class"
      ],
      "settings": {
        "foreground": "#80eaff"
      }
    },

    {
      "name": "HTML / JSX Tags",
      "scope": [
        "entity.name.tag",
        "meta.tag",
        "meta.tag.sgml",
        "support.class.component",
        "support.class.component.js",
        "support.class.component.tsx"
      ],
      "settings": {
        "foreground": "#f2858c"
      }
    },
    {
      "name": "HTML / JSX Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "variable.other.readwrite.attribute",
        "meta.tag.attributes",
        "meta.attribute",
        "meta.jsx.attribute"
      ],
      "settings": {
        "foreground": "#ded47e"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "meta.class",
        "support.class",
        "entity.other.inherited-class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#b38cff"
      }
    },

    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block.documentation",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#737780",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.ternary",
        "punctuation.separator.key-value"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },

    {
      "name": "Warnings",
      "scope": [
        "markup.warning",
        "constant.warning",
        "variable.parameter.warning"
      ],
      "settings": {
        "foreground": "#cca700"
      }
    },

    {
      "name": "Errors",
      "scope": ["invalid", "markup.error", "message.error", "keyword.error"],
      "settings": {
        "foreground": "#ff6464"
      }
    },

    {
      "name": "Default Text",
      "scope": ["source", "text"],
      "settings": { "foreground": "#e0e0e0" }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": { "foreground": "#ded47e" }
    },
    {
      "name": "CSS Properties & Values",
      "scope": [
        "support.type.property-name.css",
        "support.constant.property-value.css"
      ],
      "settings": { "foreground": "#82d99f" }
    },
    {
      "name": "HTML Attribute Numbers",
      "scope": ["constant.numeric.css", "constant.numeric.html"],
      "settings": { "foreground": "#f48cca" }
    },
    {
      "name": "JSX Text Content",
      "scope": [
        "meta.jsx.children",
        "meta.jsx.children.tsx",
        "meta.jsx.children.jsx"
      ],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "JSX Expressions",
      "scope": ["meta.block.tsx", "meta.var.expr.tsx", "meta.arrow.tsx"],
      "settings": {
        "foreground": "#e0e0e0"
      }
    },
    {
      "name": "JSX Comments",
      "scope": ["comment.block.jsx", "comment.block.tsx"],
      "settings": {
        "foreground": "#737780",
        "fontStyle": "italic"
      }
    }
  ]
}
